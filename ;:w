#include "associative_array.c"

// What follows is an easy linked-list implementation of an associative array


// Convert from raw buffer data (extracted from an inode) into an 
// associative array struct.
struct aa_array cast_to_aa(char *buff, int buff_length) {
	struct aa_array aa;

	aa.array = (struct aa_pair*) buff;
	aa.length = buff_length / sizeof(aa_pair);
	
	return aa;
}


